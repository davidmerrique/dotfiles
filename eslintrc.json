{
  "extends": "airbnb/base",
  "rules": {
    "object-curly-spacing": [2, "never"],
    "no-new": 0,
    "comma-dangle": [1, "only-multiline"],
    "max-len": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "consistent-return": 0,
    "quote-props": 0,
    "no-param-reassign": 0,
    "no-else-return": 0
  }
}
